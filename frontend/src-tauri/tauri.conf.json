{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "devPath": "Injected by vite-plugin-tauri",
    "distDir": "Injected by vite-plugin-tauri"
  },
  "package": {
    "productName": "kittygifs",
    "version": "0.4.0"
  },
  "tauri": {
    "systemTray": {
      "iconPath": "icons/icon.png",
      "iconAsTemplate": true
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.kittygifs.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "",
      "category": "Utility",
      "shortDescription": "",
      "longDescription": "",
      "deb": {
        "depends": []
      },
      "macOS": {
        "frameworks": [],
        "minimumSystemVersion": "",
        "exceptionDomain": "",
        "signingIdentity": null,
        "entitlements": null
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": "",
        "webviewInstallMode": {
          "silent": true,
          "type": "downloadBootstrapper"
        }
      }
    },
    "windows": [
      {
        "title": "kittygifs",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "label": "main",
        "visible": false
      },
      {
        "title": "kittygifs popup",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "url": "/popup",
        "label": "popup",
        "visible": false
      }
    ],
    "security": {
      "csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'",
      "dangerousRemoteDomainIpcAccess": [
        {
          "domain": "tauri.localhost",
          "windows": ["main", "popup"],
          "plugins": ["autostart", "window"]
        },
        {
          "domain": "localhost",
          "scheme": "tauri",
          "windows": ["main", "popup"],
          "plugins": ["autostart", "window"]
        }
      ]
    }
  },
  "plugins": {
    "shell": {
      "open": true
    }
  }
}
